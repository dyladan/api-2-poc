{
  "name": "api-2",
  "version": "1.0.0",
  "main": "build/cjs/index.js",
  "module": "dist/index.js",
  "types": "build/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./channel": {
      "types": "./build/types/channel.d.ts",
      "import": "./build/esm/channel.js",
      "require": "./build/cjs/channel.js"
    },
    "./context": {
      "types": "./build/types/context.d.ts",
      "import": "./build/esm/context.js",
      "require": "./build/cjs/context.js"
    },
    "./diag": {
      "types": "./build/types/diag.d.ts",
      "import": "./build/esm/diag.js",
      "require": "./build/cjs/diag.js"
    },
    "./logs": {
      "types": "./build/types/logs.d.ts",
      "import": "./build/esm/logs.js",
      "require": "./build/cjs/logs.js"
    },
    "./metrics": {
      "types": "./build/types/metrics/index.d.ts",
      "import": "./build/esm/metrics/index.js",
      "require": "./build/cjs/metrics/index.js"
    },
    "./trace": {
      "types": "./build/types/trace.d.ts",
      "import": "./build/esm/trace.js",
      "require": "./build/cjs/trace.js"
    }
  },
  "files": [
    "build/**/*.js",
    "build/**/*.js.map",
    "build/**/*.d.ts",
    "build/**/*.d.ts.map"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.types.json tsconfig.esm.json tsconfig.cjs.json",
    "postbuild": "echo '{\"type\":\"commonjs\"}' > build/cjs/package.json",
    "clean": "rm -rf build dist",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "check": "tsc --noEmit",
    "start": "tsx main.ts",
    "bundle": "webpack --config webpack.config.json"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}